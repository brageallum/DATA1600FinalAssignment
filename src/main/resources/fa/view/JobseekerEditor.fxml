<?xml version="1.0" encoding="UTF-8"?>

<?import fa.components.Editor?>
<?import fa.components.EditorTextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<Editor fx:id="editor" xmlns:fx="http://javafx.com/fxml" fx:controller="fa.JobSeekerEditorController">
    <form>
        <Label fx:id="title" />
        <EditorTextField fx:id="firstNameField" labelText="First name:" />
        <EditorTextField fx:id="lastNameField" labelText="Last name:" />
        <Button onAction="#submit" text="Submit" />
    </form>
</Editor>
